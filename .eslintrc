{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "quotes": [2, "single"],
    "strict": [2, "never"],
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/jsx-no-undef" : 2,
    "react/react-in-jsx-scope": 2,
    "import/no-extraneous-dependencies": [2, { "devDependencies": true }]
  },
  "plugins": [
    "react"
  ]
}
